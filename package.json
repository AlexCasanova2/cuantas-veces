    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "canvas-confetti": "^1.9.3",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "pinia": "^2.1.7",
    "vue": "^3.5.14",
    "vue-router": "^4.2.5"
